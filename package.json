{
    "name": "supports-hyperlinks",
    "version": "2.3.0",
    "description": "Detect if your terminal emulator supports hyperlinks",
    "license": "MIT",
    "repository": "jamestalmage/supports-hyperlinks",
    "author": {
        "name": "James Talmage",
        "email": "james@talmage.io",
        "url": "https://github.com/jamestalmage"
    },
    "type": "module",
    "exports": {
        "node": "./distribution/index.js",
        "default": "./distribution/browser.js"
    },
    "types": "distribution",
    "engines": {
        "node": "^14.18.0 || ^16.14.0 || >=18.0.0"
    },
    "scripts": {
        "build": "tsc",
        "pretest": "del-cli distribution && npm run build",
        "test": "xo && c8 ava"
    },
    "files": [
        "distribution"
    ],
    "browser": "browser.js",
    "keywords": [
        "link",
        "terminal",
        "hyperlink",
        "cli"
    ],
    "dependencies": {
        "has-flag": "^5.0.1",
        "supports-color": "^9.2.2"
    },
    "devDependencies": {
        "@sindresorhus/tsconfig": "^3.0.1",
        "@types/node": "^18.7.15",
        "ava": "^4.3.3",
        "c8": "^7.12.0",
        "del-cli": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2",
        "xo": "^0.52.3"
    },
    "ava": {
        "files": [
            "source/test.ts"
        ],
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--loader=ts-node/esm"
        ]
    }
}
